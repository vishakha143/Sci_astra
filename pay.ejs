<% layout('/layouts/boilerplate') %>

<div class="row mt-3">
  <div class="col-6 offset-2"> 
      <h3> Payment </h3>
      <br />
      <form action="/blog"  method="POST"  novalidate   class="needs-validation" enctype="multipart/form-data" >
          <div class="mb-3">
              <label for="name" class="form-label">Name</label>
              <input  
              name="name"  
              placeholder="Neha Mishra" 
              type="Add a catchy title"
              class="form-control"
              required
              />
          <div class="valid-feedback">Name required</div>
          </div>  
          <div class="mb-3">
            <label for="emai" class="form-label">Emai</label>
            <input  
            name="emai"  
            placeholder="example@example.com" 
            type="Add a catchy title"
            class="form-control"
            required
            />
          <div class="invalid-feedback">Please enter valid email</div>
          </div>
          <div class="mb-3">
              <label for="address" class="form-label">Address</label>
              <textarea
              name="address" 
              type="text"
              class="form-control"
              required
              ></textarea>
          </div>
          <div class="mb-3">
            <label for="city" class="form-label">City</label>
            <input  
            name="city"  
            placeholder="example@example.com" 
            type="text"
            class="form-control"
            required
            />
          
            <label for="state" class="form-label">State</label>
            <input  
            name="state"  
            placeholder="Germeny" 
            type="text"
            class="form-control"
            required
            />
          
            <label for="zipcode" class="form-label">Zip Code</label>
            <input  
            name="zipcode"  
            placeholder="123 456" 
            type="number"
            class="form-control"
            required
            />
          </div>
          <div class="mb-3">
            <b>PAYMENT</b>
            <div class="payment">
            <img  style="height: 50px; width: 50px;"  src="data:image/jpeg;base64," alt="gpay"/>
              <img  style="height: 50px; width: 50px;"  src="data:image/png;base64," alt="gpay"/>
            </div>
            <label for="expire_month" class="form-label">Exp.Month</label>
            <input  
            name="expire_month"  
            placeholder="August" 
            type="text"
            class="form-control"
            required
            />
          </div>
          <div class="mb-3">
            <label for="expire_year" class="form-label">Exp.Year</label>
            <input  
            name="expire_year"  
            placeholder="2025" 
            type="number"
            class="form-control"
            required
            />
            <label for="cvv" class="form-label">CVV</label>
            <input  
            name="cvv"  
            placeholder="123" 
            type="number"
            class="form-control"
            required
            />
          </div>
           <div id="liveAlertPlaceholder" class=" row row-12 mb-3">
               <button type="submit" class="btn btn-outline-primary" id="liveAlertBtn">Payment</button>
           </div>
      </form>
  </div>
</div>